@model IEnumerable<MVC1.Models.empleado>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nomE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.salud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sede.nomS)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nomE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.telE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.salud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sede.nomS)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idE }) |
            @Html.ActionLink("Details", "Details", new { id=item.idE }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idE })
        </td>
    </tr>
}

</table>
